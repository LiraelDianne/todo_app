<a href="/logout">Log Out</a>
<a href="/home">Projects Dashboard</a>
<a href="/users/<%= @user.id %>">View Your Profile</a>
<h2>All Users</h2>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Projects</th>
    <th>Actions</th>
  </tr>
<% @users.each do |u| %>
  <tr>
    <td><%= u.first_name %></td>
    <td><%= u.last_name %></td>
    <td><%= u.email %></td>
    <td>
      <ul>
        <% u.projects.each do |p| %>
        <li><%= p.name %></li>
        <% end %>
      </ul>
    </td>
    <td><a href="/users/<%= u.id %>">View</a></td>
  </tr>
<% end %>
</table>
